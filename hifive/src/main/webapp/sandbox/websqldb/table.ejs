<script type="text/ejs" id="productTable">
	[% for (var i = 0, len = result.length; i < len; i++) { %]
	<tr>
		<td class="item">[%= result[i].item %]</td>
		<td class="price">[%= result[i].price %]円</td>
		<td>
			<button class="inputThis">この品物を入力</button>
			<button class="delProduct">削除</button>
		</td>
	</tr>
	[% } %]
</script>

<script type="text/ejs" id="resultTable">
	[% var total = 0; %]
	[% for (var i = 0, len = result.length; i < len; i++) { %]
	<tr>
		<td class="date"><span class="tgl">[%= result[i].date %]</span><input type="text" maxlength=10 class="edit tgl dateInput"/></td>
		<td class="item"><span class="tgl">[%= result[i].item %]</span><input type="text" class="edit tgl itemInput"/></td>
		<td class="num"><span class="tgl">[%= result[i].num %]</span><input type="text" class="edit tgl numInput"/></td>
		<td class="price">[%= result[i].total %]円</td>
		<td>
			<button class="editResult tgl">編集</button>
			<button class="delResult tgl">削除</button>
			<button class="submitEdit edit tgl">更新</button>
			<button class="cancelEdit edit tgl">キャンセル</button>
			<input type="hidden" value="[%= result[i].key %]"/>
		</td>
	</tr>
	[% total += result[i].total %]
	[% } %]
	<tr class="total">
		<th colspan="3">合計</th>
		<td colspan="1" class="price">[%= total %]円</td>
	</tr>
</script>